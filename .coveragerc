[run]
branch = True
source = .
data_file=/tmp/coverage
omit =
    *_test.py
    *_tests.py
    .dockerignore
    Dockerfile
    docker-compose.yml
    *.txt
    *.md
    settings.py.example
    main.spec

[report]
exclude_lines =
    pragma: no cover
show_missing = False
precision = 2

[html]
directory = htmlcov
